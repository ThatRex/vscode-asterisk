{
    "name": "asterisksyntaxhighlighter",
    "displayName": "Asterisk Syntax Highlighter",
    "description": "Syntax highlighting for Asterisk dialplan",
    "version": "1.2.0",
    "publisher": "peacefixation",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "asterisk",
                "aliases": [
                    "Asterisk",
                    "asterisk"
                ],
                "extensions": [
                    ".conf"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "asterisk",
                "scopeName": "source.asterisk",
                "path": "./syntaxes/asterisk.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "asterisk",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/peacefixation/vscode-asterisk.git"
    }
}
